    <xsl:template name="show-project-overview">
        <xsl:param name="totalPages"/>
        <xsl:param name="currentPage"/>
        <xsl:param name="company1"/>
        <xsl:param name="company2"/>
        <xsl:param name="function1"/>
        <xsl:param name="function2"/>
        <xsl:param name="function3"/>
        <xsl:param name="dateFrom"/>
        <xsl:param name="dateTo"/>

        <fo:block xsl:use-attribute-sets="left-content-title">
            <xsl:value-of select="cv/translations/projecten[@lang=$language]"/>
        </fo:block>

        <fo:block xsl:use-attribute-sets="left-content">
            <xsl:call-template name="draw-company"/>&#160;
            <xsl:value-of select="$company1"/>
        </fo:block>

        <xsl:if test="$company2 != ''">
            <fo:block xsl:use-attribute-sets="left-content">
                <xsl:call-template name="draw-company"/>&#160;
                <xsl:value-of select="$company2"/>
            </fo:block>
        </xsl:if>

        <fo:block xsl:use-attribute-sets="left-content">
            <xsl:call-template name="draw-function-summary"/>&#160;
            <xsl:value-of select="$function1"/>
        </fo:block>

        <xsl:if test="$function2 != ''">
            <fo:block xsl:use-attribute-sets="left-content">
                <xsl:call-template name="draw-function-summary"/>&#160;
                <xsl:value-of select="$function2"/>
            </fo:block>
        </xsl:if>

        <xsl:if test="$function3 != ''">
            <fo:block xsl:use-attribute-sets="left-content">
                <xsl:call-template name="draw-function-summary"/>&#160;
                <xsl:value-of select="$function3"/>
            </fo:block>
        </xsl:if>

        <fo:block xsl:use-attribute-sets="left-content">
            <xsl:call-template name="draw-check-in-summary"/>&#160;
            <xsl:value-of select="$dateFrom"/>&#160;
            <xsl:call-template name="draw-check-out-summary"/>&#160;
            <xsl:value-of select="$dateTo"/>
        </fo:block>


        <fo:block xsl:use-attribute-sets="left-content">
            <xsl:call-template name="draw-page"/>&#160;
            <xsl:value-of select="cv/translations/page[@lang=$language]"/>&#160;
            <xsl:value-of select="$currentPage"/>&#160;
            <xsl:value-of select="cv/translations/pageOf[@lang=$language]"/>&#160;
            <xsl:value-of select="$totalPages"/>
        </fo:block>

    </xsl:template>

    <xsl:template name="show-project">
        <xsl:param name="job"/>
        <xsl:param name="job1"/>
        <xsl:param name="job2"/>
        <xsl:param name="job3"/>

        <xsl:call-template name="draw-job-splitter"/>

        <fo:block>
            <fo:external-graphic  height="40px" width="160px" content-height="40px"
                                  content-width="160px" src="url('src/main/resources/img/logo/{$job/logo}')">
            </fo:external-graphic>

            <xsl:if test="$job/logo2 != ''">
                <fo:external-graphic  height="40px" width="160px" content-height="40px"
                                      content-width="160px" src="url('src/main/resources/img/logo/{$job/logo2}')">
                </fo:external-graphic>
            </xsl:if>


        </fo:block>

        <fo:table table-layout="fixed" width="100%">
            <fo:table-column column-width="40%"/>
            <fo:table-column column-width="58%"/>
            <fo:table-body>
                <fo:table-row>
                    <fo:table-cell>
                        <fo:block xsl:use-attribute-sets="job-primary-no-border">
                            <xsl:call-template name="draw-function"/>&#160;
                            <xsl:value-of select="$job/title[@lang=$language]"/></fo:block>
                        <xsl:if test="$job1 != ''">
                            <fo:block xsl:use-attribute-sets="job-primary-no-border">
                                <xsl:call-template name="draw-function"/>&#160;
                                <xsl:value-of select="$job1/title[@lang=$language]"/></fo:block>
                        </xsl:if>
                        <xsl:if test="$job2 != ''">
                            <fo:block xsl:use-attribute-sets="job-primary-no-border">
                                <xsl:call-template name="draw-function"/>&#160;
                                <xsl:value-of select="$job2/title[@lang=$language]"/></fo:block>
                        </xsl:if>
                        <xsl:if test="$job3 != ''">
                            <fo:block xsl:use-attribute-sets="job-primary-no-border">
                                <xsl:call-template name="draw-function"/>&#160;
                                <xsl:value-of select="$job3/title[@lang=$language]"/></fo:block>
                        </xsl:if>
                        <fo:block xsl:use-attribute-sets="job-primary-no-border">
                            <xsl:call-template name="draw-check-in"/>&#160;
                            <xsl:if test="$job3 = ''">
                                <xsl:if test="$job2 = ''">
                                    <xsl:if test="$job1 = ''">
                                        <xsl:value-of select="$job/from"/>
                                    </xsl:if>
                                    <xsl:if test="$job1 != ''">
                                        <xsl:value-of select="$job1/from"/>
                                    </xsl:if>
                                </xsl:if>
                                <xsl:if test="$job2 != ''">
                                    <xsl:value-of select="$job2/from"/>
                                </xsl:if>
                            </xsl:if>
                            <xsl:if test="$job3 != ''">
                                <xsl:value-of select="$job3/from"/>
                            </xsl:if>&#160;
                            <xsl:call-template name="draw-check-out"/>&#160;
                            <xsl:value-of select="$job/to"/>
                        </fo:block>

                        <fo:block xsl:use-attribute-sets="job-primary-no-border">
                            <xsl:call-template name="draw-location"/>&#160;
                            <xsl:value-of select="$job/location[@lang=$language]"/></fo:block>
                    </fo:table-cell>
                    <fo:table-cell xsl:use-attribute-sets="text-with-light-background">
                        <fo:block><xsl:value-of select="$job/companyDescription[@lang=$language]"/></fo:block>
                    </fo:table-cell>
                </fo:table-row>
            </fo:table-body>
        </fo:table>
        <fo:block xsl:use-attribute-sets="job-primary">
            <xsl:call-template name="draw-tasklist"/>&#160;
            <xsl:value-of select="cv/translations/jobDescription[@lang=$language]"/>
        </fo:block>

        <fo:block xsl:use-attribute-sets="job-paragraph">
            <xsl:value-of select="$job/taskDescription[@lang=$language]"/>
        </fo:block>


        <fo:block xsl:use-attribute-sets="job-primary">
            <xsl:call-template name="draw-skill"/>&#160;
            <xsl:value-of select="cv/translations/skillTranslated[@lang=$language]"/>
        </fo:block>

        <xsl:for-each select="$job/skillSets/skillSet">
            <fo:block xsl:use-attribute-sets="job-paragraph">
                <fo:wrapper xsl:use-attribute-sets="job-paragraph-skill-header"><xsl:value-of select="skillName[@lang=$language]"/></fo:wrapper>
                <fo:wrapper>: <xsl:value-of select="skill[@lang=$language]"/></fo:wrapper>
            </fo:block>
        </xsl:for-each>

    </xsl:template>




    <xsl:template name="draw-job-splitter">
        <fo:block xsl:use-attribute-sets="job-splitter">
            <fo:block xsl:use-attribute-sets="job-splitter-line"/>
        </fo:block>
    </xsl:template>

</xsl:stylesheet>
